<script type="text/javascript">
    var _ugpi = $('.uz-g-price-item');
    if(_ugpi.size() > 0) {
        var _gids = new Array();
        _ugpi.each(function () {
           var _gid = $(this).attr('data-gid');
            if(_gid && _gid.length > 0) {
                _gids.push(_gid);
            }
        });
        _gids = array_unique(_gids);
        if(_gids.length > 0) {
        }
        $.ajax({
            url: 'http://www.btgerp.com/btgtravel/group/groupPrice/chanid/10014/type/jsonp',
            dataType: "jsonp",
            type:'get',
            async:false,
            data: {gid: _gids},
            jsonp:'callback',
            success: function (_data) {
                if(_data['data'] && _data['data'].length > 0) {
                    var _gps = _data['data'];
                    $.each(_gps, function (i, v) {
                        $('.uz-g-'+v['gid']+'-price').html(v['price']);
                    });
                }
            },
            error: function(){
            }
        });
    }


    function array_unique(arr) {
        var result = [], hash = {};
        for (var i = 0, elem; (elem = arr[i]) != null; i++) {
            if (!hash[elem]) {
                result.push(elem);
                hash[elem] = true;
            }
        }
        return result;
    }

    $(document).ready(function () {
        var _urlSearch = '/index.php?r=agentc/post/search';
        $('.c-category-list a,.ly a').each(function () {
            var obj = $(this);
            var sid=obj.attr('data-sid');
            var url='';
            if(sid){
                url = _urlSearch + '&sid=' + obj.attr('data-sid');
            }else{
                url = _urlSearch + '&keywords=' + obj.attr('data-words');
            }

            obj.attr('href', url);
            obj.attr('title', obj.text());
            obj.attr('target', '_blank');
        });

        $('.btg-c').remove();
    });
</script>
